  fieldRequest: S,
  options: RequestInit = {}
): Promise<<%= entity.api.strings.manyResponse %><S>> {
  const data = { query, fieldRequest };
  const response = await callApi('<%= `search-${utils.toKababCase(utils.pluralize(entity.name))}` %>', data, options);
  return presentManyResponse(response, fieldRequest);
}
